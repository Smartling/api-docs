  "dependencies": {
    "@redocly/cli": "^1.25.4",
    "@stoplight/json-ref-resolver": "^3.1.3",
    "@stoplight/path": "^1.3.2",
    "ansi-colors": "^4.1.1",
    "bower": "^1.8.8",
